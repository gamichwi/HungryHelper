<%= form_with(model: autocomplete, local: true) do |form| %>
  <% if autocomplete.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(autocomplete.errors.count, "error") %> prohibited this autocomplete from being saved:</h2>

      <ul>
        <% autocomplete.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :address %>
    <%= form.text_field :address %>
  </div>

  <div class="field">
    <%#= form.label :street_number %>
    <%= form.hidden_field :street_number %>
  </div>

  <div class="field">
    <%#= form.label :locality %>
    <%= form.hidden_field :locality %>
  </div>

  <div class="field">
    <%#= form.label :route %>
    <%= form.hidden_field :route %>
  </div>

  <div class="field">
    <%#= form.label :administrative_area_level_1 %>
    <%= form.hidden_field :administrative_area_level_1 %>
  </div>

  <div class="field">
    <%#= form.label :country %>
    <%= form.hidden_field :country %>
  </div>

  <div class="field">
    <%#= form.label :postal_code %>
    <%= form.hidden_field :postal_code %>
  </div>

  <div class="field">
    <%#= form.label :latitude %>
    <%= form.hidden_field :latitude %>
  </div>

  <div class="field">
    <%#= form.label :longtitude %>
    <%= form.hidden_field :longtitude %>
  </div>

  <div class="actions">
    <div id="submit" class="btn waves-effect waves-light">
      <%= form.submit "Submit" %>
    </div>
  </div>

<% end %>
