<%= form_with(model: autocomplette, local: true) do |form| %>
  <% if autocomplette.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(autocomplette.errors.count, "error") %> prohibited this autocomplette from being saved:</h2>

      <ul>
        <% autocomplette.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :address %>
    <%= form.text_field :address %>
  </div>

  <div class="field">
    <%= form.label :street_number %>
    <%= form.number_field :street_number %>
  </div>

  <div class="field">
    <%= form.label :locality %>
    <%= form.text_field :locality %>
  </div>

  <div class="field">
    <%= form.label :route %>
    <%= form.text_field :route %>
  </div>

  <div class="field">
    <%= form.label :administrative_area_level_1 %>
    <%= form.text_field :administrative_area_level_1 %>
  </div>

  <div class="field">
    <%= form.label :country %>
    <%= form.text_field :country %>
  </div>

  <div class="field">
    <%= form.label :postal_code %>
    <%= form.number_field :postal_code %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
